<suowen_api>
    <log_file>${server.home}/log/hall.api.log</log_file>
    <local_store_dir>${server.home}/config/local.api</local_store_dir>
    <web>
      <bind>0.0.0.0:${env.PORT}</bind>
    </web>
    <api>
      <database>
        <driver>org.h2.Driver</driver>
        <user>sa</user>
        <url>jdbc:h2:mem:hall</url>
      </database>
    </api>
</suowen_api>
