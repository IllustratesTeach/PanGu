<hall_image>
  <log_file>${server.home}/log/nirvana.hall.image.log</log_file>
  <local_store_dir>${server.home}/config/local</local_store_dir>
  <rpc>
    <bind>127.*:5559</bind>
  </rpc>
  <zk>
    <address>127.0.0.1:3333</address>
